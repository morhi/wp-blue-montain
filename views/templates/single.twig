{% extends 'partials/base.twig' %}

{% block content %}
    <div class="content-wrapper">
        <article class="post-type-{{post.post_type}}" id="post-{{post.ID}}">
            <section>
                <h1>{{post.title}}</h1>
                <v-text content="{{ post.content }}"></v-text>
            </section>
        </article>
    </div>
{% endblock %}