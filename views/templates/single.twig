{% extends 'partials/base.twig' %}

{% block content %}
    <div class="content-wrapper">
        <article class="post-type-{{post.post_type}}" id="post-{{post.ID}}">
            <section>
                <h1>{{post.title}}</h1>
                <div>
                    {{post.content}}
                </div>
            </section>
        </article>
    </div>
{% endblock %}